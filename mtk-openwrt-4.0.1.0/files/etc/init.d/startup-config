#!/bin/sh /etc/rc.common

# This script is startup-config using uci.
# The following commands overwrite default uci config.

START=99


stop() {
	echo "do noting..."
}

start() {
	uci set firewall.@zone[1].input='ACCEPT'
	uci set firewall.@zone[1].forward='ACCEPT'

	uci commit firewall
	/etc/init.d/firewall restart

	uci set network.lan.igmp_snooping='1'

	uci commit network
#/etc/init.d/network restart


	uci set snmpd.@system[0].sysLocation=''
	uci set snmpd.@system[0].sysContact='hepark@withusp.com'
	uci set snmpd.@system[0].sysName='cAP'
	uci set snmpd.@system[0].sysDescr='lede-mediatek-mt7622-MTK-AC4300-RFB1-squashfs-linux_4.4.124'
	uci commit snmpd
}
